{
	"name": "@bricks-ether/server-utils",
	"version": "2.0.5",
	"description": "It's my complect of utils and useful things for simple express server",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
    "nestjs.cjs",
    "nestjs.d.ts",
    "nestjs.mjs"
	],
	"main": "./dist/express.cjs",
	"module": "./dist/express.mjs",
	"types": "./dist/express/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/express.mjs",
			"default": "./dist/express.cjs",
			"types": "./dist/express/index.d.ts",
			"require": "./dist/express.cjs",
			"node": "./dist/express.cjs"
		},
		"./package.json": "./package.json",
		"./nestjs": {
			"import": "./dist/nestjs.mjs",
			"default": "./dist/nestjs.cjs",
			"types": "./dist/nestjs/index.d.ts",
			"require": "./dist/nestjs.cjs",
			"node": "./dist/nestjs.cjs"
		}
	},
	"scripts": {
		"build": "rollup --config ./rollup.config.js",
		"test": "jest",
		"test:coverage": "jest --coverage"
	},
	"keywords": [
		"server",
		"express",
		"errors",
		"utils",
		"nestjs"
	],
	"author": "Bricks667",
	"devDependencies": {
		"@nestjs/testing": "^10.1.2",
		"@rollup/plugin-commonjs": "^24.1.0",
		"@rollup/plugin-typescript": "^11.1.0",
		"@types/express": "^4.17.17",
		"@types/jest": "^29.5.3",
		"@types/multer": "^1.4.7",
		"@types/node": "^20.1.0",
		"@types/uuid": "^9.0.2",
		"babel-jest": "^29.6.1",
		"eslint-plugin-jest": "^27.2.3",
		"jest": "^29.6.1",
		"rollup": "^3.21.5",
		"ts-jest": "^29.1.1"
	},
	"dependencies": {
		"uuid": "^9.0.0"
	},
	"peerDependencies": {
		"@nestjs/common": "^10.1.2",
		"@nestjs/core": "^10.1.2",
		"@prisma/client": "^5.0.0",
		"express-validator": "^7.0.1",
		"multer": "^1.4.5-lts.1"
	}
}
